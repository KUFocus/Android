<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/box_white_14px"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/tv_check_dialog_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="다이얼로그 메시지"
        style="@style/body1B"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="26dp"/>

    <TextView
        android:id="@+id/tv_check_dialog_cancel"
        android:layout_width="116dp"
        android:layout_height="wrap_content"
        android:text="취소"
        style="@style/body2M"
        android:textColor="@color/gray400"
        android:background="@drawable/btn_grayline_30px"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@id/tv_check_dialog_title"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/tv_check_dialog_confirm"
        android:layout_marginTop="25dp"
        android:layout_marginStart="15dp"
        android:layout_marginBottom="18dp"
        android:layout_marginEnd="10dp"/>

    <TextView
        android:id="@+id/tv_check_dialog_confirm"
        android:layout_width="116dp"
        android:layout_height="wrap_content"
        android:text="확인"
        style="@style/body2M"
        android:textColor="@color/white"
        android:background="@drawable/box_blue_30px"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@id/tv_check_dialog_title"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="15dp"/>


</androidx.constraintlayout.widget.ConstraintLayout>